<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.contatos.salutecontatos"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        <activity
            android:name=".ContatoActivity"
            android:label="@string/title_activity_contato" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
       
        <activity android:label="@string/title_activity_contato" 
            	  android:name=".Contato"  >
            <intent-filter>
                <action android:name="com.contatos.salutecontatos.Contato" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:label="@string/app_name"
            android:name=".ContatoDetalhe" >
            <intent-filter >
                <action android:name="com.contatos.salutecontatos.ContatoDetalhe" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
               
    </application>

</manifest>